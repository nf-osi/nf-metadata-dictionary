{
  "$id": "https://repo-prod.prod.sagebase.org/repo/v1/schema/type/registered/org.synapse.nf-recordbasedtemplate",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RecordBasedTemplate",
  "type": "object",
  "allOf": [
    {
      "if": {
        "properties": {
          "concreteType": {
            "const": "org.sagebionetworks.repo.model.Folder"
          }
        },
        "not": {
          "properties": {
            "name": {
              "const": "Raw Data"
            }
          }
        }
      },
      "then": {
        "properties": {
          "contentType": {
            "const": "dataset"
          }
        }
      }
    }
  ]
}
