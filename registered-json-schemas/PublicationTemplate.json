{
  "$id": "https://repo-prod.prod.sagebase.org/repo/v1/schema/type/registered/org.synapse.nf-publicationtemplate",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "A generalized data model template for Publications to be hopefully used across dccs",
  "properties": {
    "abstract": {
      "type": "string"
    },
    "accessibility": {
      "description": "",
      "enum": [
        "Open Access",
        "Restricted Access"
      ],
      "title": "PublicationAccessibilityEnum",
      "type": "string"
    },
    "author": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "doi": {
      "type": "string"
    },
    "grantNumber": {
      "type": "string"
    },
    "journal": {
      "type": "string"
    },
    "pmid": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "year": {
      "type": "integer"
    }
  },
  "required": [
    "abstract",
    "author",
    "doi",
    "journal",
    "pmid",
    "title",
    "year"
  ],
  "title": "PublicationTemplate",
  "type": "object"
}