{
  "$id": "https://repo-prod.prod.sagebase.org/repo/v1/schema/type/registered/org.synapse.nf-publicationtemplate",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "A generalized data model template for Publications to be hopefully used across dccs",
  "properties": {
    "abstract": {
      "type": "string",
      "title": "abstract"
    },
    "accessibility": {
      "description": "",
      "enum": [
        "Open Access",
        "Restricted Access"
      ],
      "title": "accessibility",
      "type": "string"
    },
    "author": {
      "items": {
        "type": "string"
      },
      "type": "array",
      "title": "author"
    },
    "doi": {
      "type": "string",
      "title": "doi"
    },
    "grantNumber": {
      "type": "string",
      "title": "grantNumber"
    },
    "journal": {
      "type": "string",
      "title": "journal"
    },
    "pmid": {
      "type": "string",
      "title": "pmid"
    },
    "title": {
      "type": "string",
      "title": "title"
    },
    "year": {
      "type": "integer",
      "title": "year"
    }
  },
  "required": [
    "abstract",
    "author",
    "doi",
    "journal",
    "pmid",
    "title",
    "year"
  ],
  "title": "PublicationTemplate",
  "type": "object",
  "allOf": [
    {
      "if": {
        "properties": {
          "concreteType": {
            "const": "org.sagebionetworks.repo.model.Folder"
          }
        }
      },
      "then": {
        "properties": {
          "contentType": {
            "const": "dataset"
          }
        }
      }
    }
  ]
}
