  "title": "PortalDataset",
  "type": "object",
  "if": {
    "properties": {
      "concreteType": {
        "const": "org.sagebionetworks.repo.model.Folder"
      }
    },
    "not": {
      "properties": {
        "name": {
          "const": "Raw Data"
        }
      }
    }
  },
  "then": {
    "properties": {
      "contentType": {
        "const": "dataset"
      }
    }
